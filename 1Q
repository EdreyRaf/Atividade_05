#include <stdio.h>
#include <string.h>
int main()
{
    char nome[15];
    printf("Digete o seu nome: ");
    gets(nome);
    char sobrenome[30];
    printf("Digete o nosso sobrenome: ");
    gets(sobrenome);
    char end[30];
    printf("Digete o endereco gata: ");
    gets(end);
    char tele[10];
    printf("Digete seu telefone bb: ");
    gets(tele);
    char idade[3];
    printf("Digete a sua idade: ");
    gets(idade);
    printf("Nome: %s \nSobrenome: %s \nEndere√ßo: %s \nTelefone: %s \nIdade: %s", nome, sobrenome, end, tele, idade);
    return 0;
}
